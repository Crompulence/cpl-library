FoamFile
{
    version  2.0;
    format   ascii;
    class    dictionary;
    object   controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application      CPLSediFOAM;
startform    startTime;
startTime    0;
stopAt   endTime;
endTime     0.20;
deltaT   0.0005;
writeControl     runTime;
writeInterval    0.005;
purgeWrite   0;
writeFormat      ascii;
writePrecision   6;
writeCompression     off;
timeFormat   general;
timePrecision    6;
runTimeModifiable    true;
graphFormat      raw;

