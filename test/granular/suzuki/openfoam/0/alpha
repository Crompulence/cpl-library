FoamFile
{
    version  2.0;
    format   ascii;
    class    volScalarField;
    object   alpha;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions   [0 0 0 0 0 0 0];

internalField    uniform 0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0.523598775598;
    }
    
}
