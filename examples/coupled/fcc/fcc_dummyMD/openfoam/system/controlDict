FoamFile
{
	version	 2.0;
	format	 ascii;
	class	 dictionary;
	object	 controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application 	 cplIcoFoam;
startform 	 startTime;
startTime 	 0;
stopAt 	 endTime;
endTime 	10.1;
deltaT 	 0.1;
writeControl 	 runTime;
writeInterval 	 0.5;
purgeWrite 	 0;
writeFormat 	 ascii;
writePrecision 	 6;
writeCompression 	 off;
timeFormat 	 general;
timePrecision 	 6;
runTimeModifiable 	 true;
graphFormat 	 raw;

