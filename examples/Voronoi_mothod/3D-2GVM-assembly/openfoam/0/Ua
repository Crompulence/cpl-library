FoamFile
{
    version  2.0;
    format   ascii;
    class    volVectorField;
    object   Ua;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions   [0 1 -2 0 0 0 0];

internalField    uniform (0 0 0);

boundaryField
{
    inlet
    {
        type calculated;
	value $internalField;
    }
    outlet
    {
        type calculated;
	value $internalField;
    }
    wall1
    {
	type cyclic;
    }
    wall2
    {
	type cyclic;
    }
    wall3
    {
	type cyclic;
    }
    wall4
    {
	type cyclic;
    }
}
// ************************************************************************* //
